package kr.or.ddit.study02;

public class VarExample02 {
	public static void main(String[] args) {
		// 변수 선언
		// -> 타입 번수명;
//		int x;
//		int y;
////		int z;
////		
//		두가지 방법
////		x = 10;
////		y = 11;
////		z = 12;
////		세가지 방법
////		int x, y,z;
////		
////		x = 10;
////		y = 11;
////		z = 12;
////		
//		
		int x = 10;
		int y = 11;
		int z = 12;
		
		
		
		System.out.println("x : "+x);
		System.out.println("y : "+y);
		System.out.println("z : "+z);
		
	}

}
