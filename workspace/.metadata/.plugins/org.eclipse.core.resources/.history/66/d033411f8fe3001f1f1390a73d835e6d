package kr.or.ddit.study03.sec03;

public class CastExample02 {
	Scanner sc = new Scanner(System.in);

	public static void main(String[] args) {
		CastExample02 obj = new CastExample02();
		obj.process();

	}

	public void process() {
		method1();
		
		public void method1() {
			
			int a=20;
			int b=25;
			
			string year=""+"20"+b;
			System.out.println(year);
			
		}
		
	}

}
