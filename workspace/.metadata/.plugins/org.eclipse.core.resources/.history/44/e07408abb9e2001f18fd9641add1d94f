package kr.or.ddit.study02;

public class VarExample03 {
	public static void main(String[] args) {
		
		// 변수 이름 명명 규칙.
		// 1.  첫번째 글자는 문자이거나 $, _ 이어야 하고 숫자로
		// 		시작할 수 없다.(문자부터 와야한다.)
		// 
		// 
		// 
		// 
		// 
		// 
		// 
		int var1;
		// int 1var; 숫자 시작 불가능
		int _var;
		// _는 사용 가능하다
//		int _var;  이렇게써도 되오나 의미없이 적으면 직장가서는 혼남
//		int __var;
//		int 변수; 가능하다 하지만 한글이 깨진다.
		
//		2. 영어 대소문자를 구분.
		
		int a;
		int A;
		
//		3. 첫 문자는 영어 소문자로 시작하되.
//		다른 단어가 붙을경우 첫 문자를 대문자로 한다.
		
		int maxSpeed // 카멜 표기법(의무는 아니나 규칙이므로 지키는게 좋다.
		String carBodyColor
		
		//오라클 예시
//		MAX_SPEED
//		CAR_BODY_COLOR
		
		4. 문자 길이 제한 없다.
		5. 자바 예약어 사용할 수 없다.
		
		
		
		
		


		
	
}
}
